    <script src="https://vjs.zencdn.net/7.10.2/video.js"></script>
    <script>
        var player = videojs('my-video');

        function playStream(url, channelName) {
            player.src({ type: 'application/x-mpegURL', src: url });
            player.play(); // This will need user interaction on modern browsers
            // Update counter via a GET request
            fetch('?channel=' + encodeURIComponent(channelName)).then(response => console.log('Counter updated')).catch(error => console.error('Error updating counter:', error));
        }

        // Optionally, load the first channel by default
        playStream('https://glb.bozztv.com/glb/ushba0/index.m3u8', 'ushba0');
    </script>
